<?xml version="1.0" encoding="UTF-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background" android:drawable="@drawable/seekbar_bg" />
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <nine-patch android:src="@drawable/seekbar_sec_progress_bg" android:dither="true"
              xmlns:android="http://schemas.android.com/apk/res/android" />
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <nine-patch android:src="@drawable/seekbar_progress_bg" android:dither="true"
              xmlns:android="http://schemas.android.com/apk/res/android" />
        </clip>
    </item>
</layer-list>